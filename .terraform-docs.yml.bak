version: ">= 0.14.0"

formatter: markdown

header-from: main.tf
footer-from: ""

sections:
  hide: []
  show: 
    - header
    - footer
    - inputs
    - outputs
    - providers
    - requirements

content: ""

output:
  file: "README.md"
  mode: inject
  template: |-
    <!-- Begin Terraform Docs -->
    {{ .Content }}
    <!-- End Terraform Docs -->

output-values:
  enabled: false
  from: ""

settings:
  anchor: true
  color: true
  default: true
  description: false
  escape: true
  html: true
  indent: 2
  required: true
  sensitive: true
  type: true
